class UserMainCode
{
    public int sumOfSumsOfDigits(int input1)
    {
        String s = String.valueOf(input1);
        int n = s.length();
        int totalSum = 0;

        for (int i = 0; i < n; i++) {
            int partialSum = 0;
            for (int j = i; j < n; j++) {
                partialSum += s.charAt(j) - '0';
            }
            totalSum += partialSum;
        }

        return totalSum;
    }
}
