class UserMainCode
{
    public int isPalindrome(String input1)
    {
        String s = input1.toLowerCase();
        int left = 0;
        int right = s.length() - 1;

        while (left < right) {
            if (s.charAt(left) != s.charAt(right)) {
                return 1;   // NOT a palindrome
            }
            left++;
            right--;
        }

        return 2;   // Palindrome
    }
}
